{
  "expo": {
    "name": "FindMovies",
    "slug": "FindMovies",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./source/assets/find_movies.png",
    "splash": {
      "image": "./source/assets/find_movies.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "foregroundImage": "./source/assets/find_movies.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./source/assets/find_movies.png"
    }
  }
}
